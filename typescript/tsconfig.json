{
	"compilerOptions": {
		"target": "ES2016",
		"module": "commonjs",
		"sourceMap": true,
		"lib": ["ES5", "ES2015.Promise", "ES2016.Array.Include", "ES2015", "DOM"],
		"strict": true,
		"noImplicitAny": true,
		"removeComments": true,
		"noEmitOnError": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"outDir": "dist"
	}
}
